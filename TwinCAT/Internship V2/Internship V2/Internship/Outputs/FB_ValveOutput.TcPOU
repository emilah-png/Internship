<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.13">
  <POU Name="FB_ValveOutput" Id="{b39826ab-3b1e-4df6-8ee9-5d26d1d1ff9d}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_ValveOutput
VAR_INPUT
	bEnable 			: BOOL;
	fNormalizedInput 	: LREAL;
END_VAR
VAR_OUTPUT
	nOutput : INT; 
END_VAR
VAR
	nOffset 	: INT := 19630;
	nMaxValue	: INT := 29460;
	nMinValue 	: INT :=  9830;
	nTempOutput : INT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
IF fNormalizedInput > 0.0 THEN
	nTempOutput := LREAL_TO_INT(fNormalizedInput) * (nMaxValue - nOffset) + nOffset;
ELSIF fNormalizedInput < 0.0 THEN
	nTempOutput := LREAL_TO_INT(fNormalizedInput) * (nOffset - nMinValue) + nOffset;
ELSE 
	nTempOutput := nOffset;
END_IF



IF NOT(bEnable) THEN
	nOutput := nOffset;
ELSIF (bEnable) THEN
	nOutput := nTempOutput;	
END_IF]]></ST>
    </Implementation>
